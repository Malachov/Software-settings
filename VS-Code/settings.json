{
  "launch": {
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python: Current File",
        "type": "python",
        "request": "launch",
        "program": "${file}",
        "console": "internalConsole",
        "justMyCode": false
      },
      {
        "name": "Debug Tests",
        "type": "python",
        "request": "test",
        "console": "integratedTerminal",
        "justMyCode": false
      }
    ]
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[vue]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  },
  "[yaml]": {
    "editor.tabSize": 2
  },
  "autoDocstring.docstringFormat": "google",
  "breadcrumbs.enabled": true,
  "code-runner.executorMap": {
    "python": "$pythonPath $fullFileName"
  },
  "code-runner.runInTerminal": true,
  "commentAnchors.workspace.enabled": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.autoClosingBrackets": "never",
  "editor.detectIndentation": false,
  "editor.folding": true,
  "editor.fontSize": 14,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.formatOnType": false,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": true,
  "editor.quickSuggestionsDelay": 0,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "none",
  "editor.showFoldingControls": "always",
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 4,
  //   "editor.wordWrap": "on",
  "files.associations": {
    "*.js": "javascript"
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.suggestSmartCommit": false,
  "gitlens.views.repositories.branches.layout": "list",
  "gitlens.hovers.annotations.enabled": false,
  "gitlens.hovers.annotations.details": false,
  "gitlens.currentLine.enabled": false, // in modern version
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "go.useLanguageServer": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "jupyter.askForKernelRestart": false,
  "jupyter.interactiveWindowMode": "perFile",
  "jupyter.textOutputLimit": 100000,
  "liveServer.settings.port": 0,
  "liveshare.authenticationProvider": "GitHub",
  "markdown.preview.breaks": true,
  "markdown.preview.fontSize": 10,
  "markdown.preview.lineHeight": 1,
  "npm.autoDetect": "on",
  "npm.includeDirectories": ["gui"],
  "prettier.printWidth": 100,
  "prettier.bracketSpacing": true,
  "python.formatting.blackArgs": ["--line-length", "110"],
  "python.formatting.provider": "black",
  "python.jediMemoryLimit": -1,
  "python.languageServer": "Pylance",
  "python.linting.flake8Args": ["--ignore=E303, E501, E225, E266, E402, E265", "--verbose"],
  "python.linting.flake8Enabled": true,
  "python.linting.pylintEnabled": false,
  "python.testing.unittestEnabled": false,
  "python.testing.nosetestsEnabled": false,
  "python.testing.pytestEnabled": true,
  "python.testing.autoTestDiscoverOnSaveEnabled": false,
  "python.testing.pytestArgs": ["-vv", "--vv", "--color=yes"],
  "restructuredtext.builtDocumentationPath": "${workspaceRoot}/docs/source/_build/html",
  "restructuredtext.confPath": "${workspaceRoot}/docs/source",
  "rewrap.wrappingColumn": 110,
  "terminal.external.linuxExec": "/usr/bin/gnome-terminal",
  "vetur.ignoreProjectWarning": true,
  "workbench.activityBar.visible": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "terminal.external.windowsExec": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
  "python.showStartPage": false,
  "tabnine.experimentalAutoImports": true,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "mssql.connections": [
    {
      "server": "{{put-server-name-here}}",
      "database": "{{put-database-name-here}}",
      "user": "{{put-username-here}}",
      "password": ""
    },
    {
      "server": ".",
      "database": "DemoData",
      "authenticationType": "SqlLogin",
      "user": "sa",
      "password": "",
      "emptyPasswordInput": false,
      "savePassword": true
    }
  ],
  "window.zoomLevel": 1,
  "timeline.excludeSources": ["git-history"]
}
