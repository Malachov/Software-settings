{
  "python.venvPath": "${workspaceRoot}/venv",
  "launch": {
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python: As module",
        "type": "python",
        "request": "launch",
        "module": "${relativeFileDirname}.${fileBasenameNoExtension}", // If submodule, use "${relativeFileDirname}.MYSUBMODULE.${fileBasenameNoExtension}"
        "console": "internalConsole",
        "justMyCode": false // Not working here
      },
      {
        "name": "Python: Tests",
        "type": "python",
        "request": "launch",
        "program": "${file}",
        "purpose": ["debug-test"],
        "console": "integratedTerminal",
        "justMyCode": false
      },
      {
        "name": "Python: Current File",
        "type": "python",
        "request": "launch",
        "program": "${file}",
        "console": "integratedTerminal",
        "justMyCode": false
      }
    ]
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  //   "[xml]": {
  //     "editor.defaultFormatter": "DotJoshJohnson.xml"
  //   },
  "[yaml]": {
    "editor.tabSize": 2
  },
  "autoDocstring.docstringFormat": "google",
  "breadcrumbs.enabled": true,
  "code-runner.runInTerminal": true,
  "code-runner.executorMap": {
    "python": "$pythonPath $fullFileName"
  },
  "commentAnchors.workspace.enabled": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.autoClosingBrackets": "never",
  "editor.bracketPairColorization.enabled": true,
  "editor.detectIndentation": false,
  "editor.folding": true,
  "editor.fontFamily": "Hack Nerd Font Mono",
  "editor.fontSize": 14,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.formatOnType": false,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": true,
  "editor.quickSuggestionsDelay": 0,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "none",
  "editor.rulers": [110],
  "editor.showFoldingControls": "always",
  "editor.snippetSuggestions": "bottom",
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.suggest.showKeywords": false,
  "editor.tabCompletion": "on",
  "editor.tabSize": 4,
  //   "editor.wordWrap": "on",
  "emmet.triggerExpansionOnTab": true,
  "files.associations": {
    "*.js": "javascript"
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.suggestSmartCommit": false,
  "gitlens.hovers.annotations.changes": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false, // in modern version
  "gitlens.hovers.annotations.details": false,
  "gitlens.hovers.annotations.enabled": false,
  "gitlens.views.repositories.branches.layout": "list",
  "go.useLanguageServer": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "jupyter.askForKernelRestart": false,
  "jupyter.interactiveWindowMode": "perFile",
  "languageToolLinter.external.url": "http://localhost:8082",
  "languageToolLinter.languageTool.language": "en",
  "languageToolLinter.serviceType": "public",
  "languageToolLinter.languageTool.disabledRules": "WHITESPACE_RULE",
  "liveServer.settings.port": 0,
  "liveshare.authenticationProvider": "GitHub",
  "markdown.preview.breaks": true,
  "markdown.preview.fontSize": 10,
  "markdown.preview.lineHeight": 1,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "notebook.output.textLineLimit": 1000,
  "npm.autoDetect": "on",
  "npm.includeDirectories": ["gui"],
  "prettier.bracketSpacing": true,
  "prettier.printWidth": 110,
  "python.analysis.typeCheckingMode": "basic",
  "python.analysis.autoImportCompletions": false,
  "python.formatting.blackArgs": ["--line-length", "110"],
  "python.formatting.provider": "black",
  "python.languageServer": "Pylance",
  "python.linting.flake8Enabled": false,
  "python.linting.pylintArgs": [
    "--disable",
    "wrong-import-position, import-outside-toplevel, fixme, unspecified-encoding",
    "--max-line-length",
    "110",
    "--good-names",
    "df, i, j, T, U, f, a, b, c, n",
    "--load-plugins",
    "perflint"
    // "--ignore-patterns",
    // "[**/test_readme_generated*, venv/**]"
  ], // "--ignore=E303, E501, E225, E266, E402, E265"
  "python.linting.pydocstyleEnabled": true,
  "python.linting.pydocstyleArgs": ["--convention=google", "--match=^((?!test_).)*$"],
  "python.linting.banditEnabled": true,
  "python.linting.banditArgs": ["--exclude_dirs=tests"],
  "python.linting.ignorePatterns": ["**/site-packages", ".vscode", "**/Lib", "**/dist", "**/build"],
  "cSpell.spellCheckOnlyWorkspaceFiles": true,
  "cSpell.ignorePaths": [
    "**/site-packages/*",
    ".vscode/*.py",
    "**/Lib/*",
    "**/dist/*",
    "**/build/*",
    "**/test_readme_generated*"
  ],
  "python.testing.pytestEnabled": true,
  "python.testing.unittestEnabled": false,
  "esbonio.sphinx.buildDir": "${workspaceRoot}/docs/build/html",
  "esbonio.sphinx.confDir": "${workspaceRoot}/docs/source/conf.py",
  "esbonio.server.sourceFolder": "${workspaceRoot}/docs/source",
  "esbonio.server.enabled": false,
  //   "restructuredtext.preview.name": "sphinx",
  "rewrap.wrappingColumn": 110,
  "terminal.external.linuxExec": "/usr/bin/gnome-terminal",
  "terminal.integrated.allowMnemonics": true,
  "terminal.integrated.fontFamily": "Hack Nerd Font Mono",
  "testing.automaticallyOpenPeekView": "failureAnywhere",
  "testing.followRunningTest": true,
  "vetur.ignoreProjectWarning": true,
  "window.restoreWindows": "all",
  "workbench.activityBar.visible": true,
  "workbench.colorTheme": "Visual Studio Dark",
  "workbench.colorCustomizations": {
    "scrollbarSlider.background": "#b45858ad"
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "taskExplorer.enableBash": false,
  "taskExplorer.enableBatch": false,
  "taskExplorer.enableMake": false,
  "taskExplorer.enablePowershell": false,
  "taskExplorer.enableGradle": false,
  "taskExplorer.enableGrunt": false,
  "taskExplorer.enableMaven": false,
  "taskExplorer.useAnt": false,
  "taskExplorer.useGulp": false,
  "taskExplorer.enablePython": false,
  "taskExplorer.enableWorkspace": false,
